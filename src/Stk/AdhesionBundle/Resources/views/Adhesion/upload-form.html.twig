{% extends "StkAdhesionBundle::layout.html.twig" %}
{% block body_adhesion %}
    <h2>Importé des données de membres</h2>
    {% if error_message is defined %}
        <p style="color: red "> {{ error_message }}</p>
    {% endif %}
    <form method="post" enctype="multipart/form-data">
        <table class="table-form">
            <tr>
                <td>{{ form_label(form.attachement) }}</td>
                <td>{{ form_widget(form.attachement) }}</td>
            </tr>
        </table>
        <div class="form-button">
            <input type="submit" value="Enregistrer"/>
        </div>
    </form>
{% endblock %}
{% block javascripts %}
    <script>
        var nav = document.getElementById("adhesion-new-import-link");
        nav.className += " active";
    </script>
{% endblock %}